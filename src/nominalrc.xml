<?xml version="1.0" ?>
<emulator xmlns="http://www.srpalmer.me.uk/ns/emulator" version="0.05">
<!-- Copyright 2016 Steve Palmer -->

  <computer name="nominal_atom" extends="minimal_atom">
    <ram name="video">
      <size>#1800</size>
    </ram>
    <address_space>
      <map>
        <base>#2800</base>
        <ram name="lower">
          <size>#1400</size>
        </ram>
      </map>
      <map>
        <base>#D000</base>
        <rom name="float">
          <filename>float.rom</filename>
        </rom>
      </map>
    </address_space>
  </computer>

</emulator>
